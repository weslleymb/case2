{
	"name": "curated",
	"properties": {
		"activities": [
			{
				"name": "marca_linha",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "EXEC dbo.pr_cria_tabelas @dssqldb_tabela = 'marca_linha'",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "dsdlsparquet",
						"type": "DatasetReference",
						"parameters": {
							"dsdlsparquet_diretorio": "silver"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "dssqldb",
						"type": "DatasetReference",
						"parameters": {
							"dssqldb_tabela": "marca_linha"
						}
					}
				]
			}
		],
		"folder": {
			"name": "Curated"
		},
		"annotations": []
	}
}