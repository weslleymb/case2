{
	"name": "dsdls",
	"properties": {
		"linkedServiceName": {
			"referenceName": "lsdls",
			"type": "LinkedServiceReference",
			"parameters": {
				"data_lake_container": "raw"
			}
		},
		"parameters": {
			"dsdls_container": {
				"type": "string"
			},
			"dsdls_arquivo": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "DelimitedText",
		"typeProperties": {
			"location": {
				"type": "AzureBlobFSLocation",
				"fileName": {
					"value": "@concat(replace(dataset().dsdls_arquivo, ' ', '_'), '.txt')",
					"type": "Expression"
				},
				"fileSystem": {
					"value": "@dataset().dsdls_container",
					"type": "Expression"
				}
			},
			"columnDelimiter": ",",
			"escapeChar": "\\",
			"firstRowAsHeader": true,
			"quoteChar": "\""
		},
		"schema": []
	}
}