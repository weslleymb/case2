{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-case2-dev-eastus"
		},
		"lsdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdls'"
		},
		"lsdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscase2deveastus.dfs.core.windows.net"
		},
		"lsfsexcel_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "@{linkedService().lsfsexcel_diretorio_on_prem}"
		},
		"lsfsexcel_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "@{linkedService().lsfsexcel_usuario_on_prem}"
		},
		"lskeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-case2-dev-eastus.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-case2-dev-eastus_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lsdls_accountKey": {
						"value": "[parameters('lsdls_accountKey')]"
					},
					"lskeyvault_properties_typeProperties_baseUrl": {
						"value": "[parameters('lskeyvault_properties_typeProperties_baseUrl')]"
					},
					"lsfsexcel_properties_typeProperties_host": {
						"value": "[parameters('lsfsexcel_properties_typeProperties_host')]"
					},
					"lsfsexcel_properties_typeProperties_userId": {
						"value": "[parameters('lsfsexcel_properties_typeProperties_userId')]"
					},
					"lsdls_properties_typeProperties_url": {
						"value": "[parameters('lsdls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}