{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-case2-dev-eastus"
		},
		"lsdls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'lsdls'"
		},
		"lsdls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlscase2deveastus.dfs.core.windows.net"
		},
		"lsfsexcel_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "@{linkedService().on_prem_dir}"
		},
		"lsfsexcel_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "@{linkedService().on_prem_user}"
		},
		"lskeyvault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-case2-dev-eastus.vault.azure.net/"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/raw_import_files')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy data1",
						"type": "Copy",
						"dependsOn": [],
						"policy": {
							"timeout": "7.00:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "ExcelSource",
								"storeSettings": {
									"type": "FileServerReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								}
							},
							"sink": {
								"type": "DelimitedTextSink",
								"storeSettings": {
									"type": "AzureBlobFSWriteSettings"
								},
								"formatSettings": {
									"type": "DelimitedTextWriteSettings",
									"quoteAllText": true,
									"fileExtension": ".txt"
								}
							},
							"enableStaging": false,
							"translator": {
								"type": "TabularTranslator",
								"mappings": [
									{
										"source": {
											"name": "ID_MARCA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ID_MARCA",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "MARCA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "MARCA",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "ID_LINHA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "ID_LINHA",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "LINHA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "LINHA",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "DATA_VENDA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "DATA_VENDA",
											"type": "String",
											"physicalType": "String"
										}
									},
									{
										"source": {
											"name": "QTD_VENDA",
											"type": "String",
											"physicalType": "String"
										},
										"sink": {
											"name": "QTD_VENDA",
											"type": "String",
											"physicalType": "String"
										}
									}
								],
								"typeConversion": true,
								"typeConversionSettings": {
									"allowDataTruncation": true,
									"treatBooleanAsNumber": false
								}
							}
						},
						"inputs": [
							{
								"referenceName": "dsfsexcel",
								"type": "DatasetReference",
								"parameters": {}
							}
						],
						"outputs": [
							{
								"referenceName": "dsdls",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					}
				],
				"policy": {
					"elapsedTimeMetric": {},
					"cancelAfter": {}
				},
				"variables": {
					"user_on_prem": {
						"type": "String",
						"defaultValue": "w.de.moura.barbosa@avanade.com"
					},
					"source_on_prem": {
						"type": "String",
						"defaultValue": "C:\\work\\grupo_boticario"
					},
					"source_process_on_prem": {
						"type": "String",
						"defaultValue": "C:\\work\\grupo_boticario\\processados"
					}
				},
				"folder": {
					"name": "raw"
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/dsfsexcel')]",
				"[concat(variables('factoryId'), '/datasets/dsdls')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dsdls')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "lsdls",
					"type": "LinkedServiceReference"
				},
				"annotations": [],
				"type": "DelimitedText",
				"typeProperties": {
					"location": {
						"type": "AzureBlobFSLocation",
						"fileSystem": "raw"
					},
					"columnDelimiter": ",",
					"escapeChar": "\\",
					"firstRowAsHeader": true,
					"quoteChar": "\""
				},
				"schema": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/lsdls')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/dsfsexcel')]",
			"type": "Microsoft.DataFactory/factories/datasets",
			"apiVersion": "2018-06-01",
			"properties": {
				"linkedServiceName": {
					"referenceName": "lsfsexcel",
					"type": "LinkedServiceReference",
					"parameters": {
						"on_prem_user": "w.de.moura.barbosa@avanade.com",
						"on_prem_dir": "C:\\\\work\\\\grupo_boticario"
					}
				},
				"annotations": [],
				"type": "Excel",
				"typeProperties": {
					"sheetName": "Planilha1",
					"location": {
						"type": "FileServerLocation",
						"fileName": "Base 2017.xlsx"
					},
					"firstRowAsHeader": true
				},
				"schema": [
					{
						"name": "ID_MARCA",
						"type": "String"
					},
					{
						"name": "MARCA",
						"type": "String"
					},
					{
						"name": "ID_LINHA",
						"type": "String"
					},
					{
						"name": "LINHA",
						"type": "String"
					},
					{
						"name": "DATA_VENDA",
						"type": "String"
					},
					{
						"name": "QTD_VENDA",
						"type": "String"
					}
				]
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/linkedServices/lsfsexcel')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsdls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"data_lake_container": {
						"type": "string",
						"defaultValue": "raw"
					}
				},
				"annotations": [],
				"type": "AzureBlobFS",
				"typeProperties": {
					"url": "[parameters('lsdls_properties_typeProperties_url')]",
					"accountKey": {
						"type": "SecureString",
						"value": "[parameters('lsdls_accountKey')]"
					}
				},
				"connectVia": {
					"referenceName": "AutoResolveIntegrationRuntime",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/AutoResolveIntegrationRuntime')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lsfsexcel')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"on_prem_user": {
						"type": "string",
						"defaultValue": "w.de.moura.barbosa@avanade.com"
					},
					"on_prem_dir": {
						"type": "string",
						"defaultValue": "C:\\\\work\\\\grupo_boticario"
					}
				},
				"annotations": [],
				"type": "FileServer",
				"typeProperties": {
					"host": "[parameters('lsfsexcel_properties_typeProperties_host')]",
					"userId": "[parameters('lsfsexcel_properties_typeProperties_userId')]",
					"password": {
						"type": "AzureKeyVaultSecret",
						"store": {
							"referenceName": "lskeyvault",
							"type": "LinkedServiceReference"
						},
						"secretName": "pass-user-on-prem"
					}
				},
				"connectVia": {
					"referenceName": "ircase2",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/integrationRuntimes/ircase2')]",
				"[concat(variables('factoryId'), '/linkedServices/lskeyvault')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/lskeyvault')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureKeyVault",
				"typeProperties": {
					"baseUrl": "[parameters('lskeyvault_properties_typeProperties_baseUrl')]"
				}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/AutoResolveIntegrationRuntime')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "AutoResolve",
						"dataFlowProperties": {
							"computeType": "General",
							"coreCount": 8,
							"timeToLive": 0
						}
					}
				},
				"managedVirtualNetwork": {
					"type": "ManagedVirtualNetworkReference",
					"referenceName": "default"
				}
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/managedVirtualNetworks/default')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/ircase2')]",
			"type": "Microsoft.DataFactory/factories/integrationRuntimes",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "SelfHosted",
				"typeProperties": {}
			},
			"dependsOn": []
		},
		{
			"name": "[concat(parameters('factoryName'), '/default')]",
			"type": "Microsoft.DataFactory/factories/managedVirtualNetworks",
			"apiVersion": "2018-06-01",
			"properties": {},
			"dependsOn": []
		}
	]
}